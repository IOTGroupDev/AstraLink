{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && tsc-alias -p tsconfig.build.json",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "rebuild": "npm run clean && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_OPTIONS='-r tsconfig-paths/register' nest start",
    "start:dev": "NODE_OPTIONS='-r tsconfig-paths/register' nest start --watch",
    "start:debug": "NODE_OPTIONS='-r tsconfig-paths/register' nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "seed:dating": "ts-node -r tsconfig-paths/register src/scripts/seed.dating.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.16.1",
    "@supabase/supabase-js": "^2.81.1",
    "@types/compression": "^1.8.1",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "nestjs-i18n": "^10.5.1",
    "nodemailer": "^7.0.9",
    "openai": "^6.9.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.16.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sanitize-html": "^2.17.0",
    "swisseph": "^0.5.17",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^3.15.9",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.5.0",
    "jest": "^30.0.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "overrides": {
    "js-yaml": "^4.1.1",
    "tmp": "^0.2.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
