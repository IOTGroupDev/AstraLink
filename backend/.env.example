# Database
DATABASE_URL="postgresql://user:password@localhost:5432/astralink"

# JWT
JWT_SECRET="your-super-secure-jwt-secret-here"
JWT_REFRESH_SECRET="your-refresh-token-secret"

# Supabase
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# AI Services
OPENAI_API_KEY="your-openai-key"
ANTHROPIC_API_KEY="your-anthropic-key"
DEEPSEEK_API_KEY="your-deepseek-key"
# AI Provider Preference: 'claude' | 'openai' | 'deepseek' | 'auto' (default: auto)
# - 'claude': Use Claude as primary provider (premium quality, $3/1M in)
# - 'openai': Use OpenAI as primary provider (good quality, $0.15/1M in)
# - 'deepseek': Use DeepSeek as primary provider (best value, $0.14/1M in)
# - 'auto': Automatic selection (Claude > DeepSeek > OpenAI priority)
AI_PROVIDER_PREFERENCE="auto"

# Redis
REDIS_URL="redis://localhost:6379"

# Environment
NODE_ENV="development"