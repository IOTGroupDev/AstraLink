{
  "expo": {
    "name": "Astralink",
    "slug": "astralink",
    "scheme": "astralink",
    "version": "1.0.0",
    "orientation": "portrait",
    "main": "index.ts",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "astralink.test",
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "buildNumber": "1",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Используется для безопасного входа в приложение"
      }
    },
    "android": {
      "package": "astralink.test",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0618"
      },
      "permissions": ["USE_BIOMETRIC", "USE_FINGERPRINT"],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "astralink",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id",
        "SUPABASE_URL": "https://ayoucajwdyinyhamousz.supabase.co",
        "SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImF5b3VjYWp3ZHlpbnloYW1vdXN6Iiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTg3MDcyMDcsImV4cCI6MjA3NDI4MzIwN30.S-JOt3sVAEzbZTIEJrHDsKthp3pA5wGsyNEfHfeOrHo",
        "API_URL": "https://api.astralink.vip/api"
      }
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-web-browser",
      "expo-local-authentication",
      "expo-apple-authentication"
    ]
  }
}
